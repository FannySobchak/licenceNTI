<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="NtiBundle\Entity\Etudiant" table="etudiant">
    <indexes>
      <index name="FK_etudiant_id_session" columns="id_session"/>
    </indexes>
    <id name="idUser" association-key="true"/>
    <one-to-one field="idUser" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="id_user" referenced-column-name="id_user"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="idSession" target-entity="Session" fetch="LAZY">
      <join-columns>
        <join-column name="id_session" referenced-column-name="id_session"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
