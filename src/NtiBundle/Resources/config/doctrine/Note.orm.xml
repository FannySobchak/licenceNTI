<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="NtiBundle\Entity\Note" table="note">
    <unique-constraints>
      <unique-constraint name="id_epreuve" columns="id_epreuve"/>
      <unique-constraint name="id_session" columns="id_session"/>
      <unique-constraint name="id_user" columns="id_user"/>
    </unique-constraints>
    <id name="idNote" type="integer" column="id_note">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="note" type="float" column="note" precision="10" scale="0" nullable="false"/>
    <many-to-one field="idUser" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="id_user" referenced-column-name="id_user"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idSession" target-entity="Session" fetch="LAZY">
      <join-columns>
        <join-column name="id_session" referenced-column-name="id_session"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEpreuve" target-entity="Epreuve" fetch="LAZY">
      <join-columns>
        <join-column name="id_epreuve" referenced-column-name="id_epreuve"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
