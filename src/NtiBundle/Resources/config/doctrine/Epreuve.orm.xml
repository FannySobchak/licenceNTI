<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="NtiBundle\Entity\Epreuve" table="epreuve">
    <indexes>
      <index name="FK_epreuve_id_cours" columns="id_cours"/>
    </indexes>
    <id name="idEpreuve" type="integer" column="id_epreuve">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateepreuve" type="date" column="dateEpreuve" nullable="false"/>
    <field name="intitule" type="string" column="intitule" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idCours" target-entity="Cours" fetch="LAZY">
      <join-columns>
        <join-column name="id_cours" referenced-column-name="id_cours"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
