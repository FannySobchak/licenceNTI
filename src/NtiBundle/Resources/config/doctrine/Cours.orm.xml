<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="NtiBundle\Entity\Cours" table="cours">
    <id name="idCours" type="integer" column="id_cours">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelle" type="string" column="libelle" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="idUser" target-entity="User" mapped-by="idCours" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
